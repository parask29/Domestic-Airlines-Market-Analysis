   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2110 are present in duration\n",
      "No Outliers in days_left\n",
      "123 are present in price\n"
     ]
    }
   ],
   "source": [
    "## Outliers Count --  IN All num_feat\n",
    "## Counting of Outliers\n",
    "for i in Num_feat:\n",
    "    Q1 = df[i].quantile(0.25)\n",
    "    Q3 = df[i].quantile(0.75)\n",
    "    IQR = Q3-Q1\n",
    "    Lower_bound = Q1-1.5*IQR\n",
    "    Upper_bound = Q3+1.5*IQR\n",
    "    outliers = df[(df[i]< Lower_bound) | (df[i]>Upper_bound)][i]\n",
    "    len_outliers = len(outliers)\n",
    "    if len(outliers) == 0:\n",
    "        print(f'No Outliers in {i}')\n",
    "    else:\n",
    "        print(f'{len_outliers} are present in {i}')\n",
    "\n"
   ]
  },
